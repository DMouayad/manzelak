{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@localization/*": [
        "src/i18n/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@scripts/*": [
        "src/assets/scripts/*"
      ],
      "@styles/*": [
        "src/assets/scss/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
  }
}